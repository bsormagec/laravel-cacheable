{
    "name": "sormagec/laravel-mongo-cacheable",
    "description": "Sormagec Mongo Cacheable is a granular, intuitive, and fluent caching system for eloquent mongodb models. Simple, but yet powerful, plug-n-play with no hassle.",
    "type": "library",
    "keywords": [
        "trait",
        "model",
        "laravel",
        "eloquent",
        "intuitive",
        "cacheable",
        "granular",
        "sormagec",
        "fluent",
        "query",
        "cache"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/cache": "~5.6.0",
        "illuminate/contracts": "~5.6.0",
        "illuminate/database": "~5.6.0",
        "jenssegers/mongodb": "^3.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.23.0",
        "illuminate/container": "~5.6.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sormagec\\Cacheable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sormagec\\Cacheable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
